
(load-stencil :grid)

(create-svg (:width 220 :height 50 :desc "sample of stroke-linejoin" :encoding :utf8)
	(grid)
	(let ((*default-stroke* (make-stroke :color :black :width 12)))
	  (line '( 30 30  45 15  60 30) :stroke '(:linejoin :miter))
	  (line '( 90 30 105 15 120 30) :stroke '(:linejoin :round))
	  (line '(150 30 165 15 180 30) :stroke '(:linejoin :bevel))))
